{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": [
      "ES2016",
      "DOM",
      "DOM.Iterable",
      "DOM.AsyncIterable"
    ],
    "noImplicitAny": false,
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@images/*": [
        "src/assets/images/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    },
    "target": "ES2016"
  },
  "extends": "astro/tsconfigs/base"
}