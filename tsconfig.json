{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "allowJs": true,
    "noImplicitAny": false,
    "target": "ES6",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@images/*": [
        "src/assets/images/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@data/*": [
        "src/data/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}